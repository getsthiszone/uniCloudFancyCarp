page {
  background-color: #FFF;
}
.ciew {
  text-align: center;
  position: relative;
  z-index: 1;
}
.ciew image {
  width: 695rpx;
}
.list {
  width: 675rpx;
  background-color: #fff4ee;
  -webkit-border-radius: 0rpx 0rpx 20rpx 20rpx;
          border-radius: 0rpx 0rpx 20rpx 20rpx;
  margin: auto;
  margin-top: -20rpx;
  padding-bottom: 20rpx;
}
.list .vtitle {
  font-size: 28rpx;
  color: #ff6c13;
  height: 92rpx;
  line-height: 92rpx;
  border-bottom: dashed 1rpx #ff6c13;
  text-align: center;
}
.topview {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  margin: auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  width: 695rpx;
  height: 148rpx;
  color: #FFF;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  z-index: 99;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-border-radius: 20rpx 20rpx 0rpx 0rpx;
          border-radius: 20rpx 20rpx 0rpx 0rpx;
}
.coupontop {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  position: relative;
  height: 175rpx;
  padding: 0rpx 30rpx;
}
.coupontop .comment {
  width: 430rpx;
  height: 110rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.coupontop .comment .title {
  font-size: 28rpx;
  color: #2f2f2f;
}
.coupontop .comment .endtime {
  font-size: 24rpx;
  color: #9d9d9d;
}
.coupontop .rprice {
  height: 110rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  text-align: center;
}
.coupontop .rprice .price {
  font-size: 46rpx;
  color: #ff0e0e;
  font-weight: 600;
}
.coupontop .rprice .price .price-icon {
  font-size: 31rpx;
}
.coupontop .rprice .subtitle {
  font-size: 24rpx;
  color: #9d9d9d;
}

