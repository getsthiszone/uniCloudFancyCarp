}
.goodslist .goods-dview .goods-price {
	font-size: 29rpx;
	color: #ff0101;
}
.goodslist .goods-dview .icon-car image {
	width: 45rpx;
	height: 41rpx;
}
.goods-dview .icon-car {
	font-size: 45rpx;
	color: #ff4c29;
}

